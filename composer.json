{
  "name": "ppito/zf3-sass-compiler",
  "description": "ZF3-sass-compiler, integrated scssphp in ZF3 Framework",
  "type": "module",
  "license": "MIT License",
  "homepage": "https://github.com/ppito/zf3-sass-compiler",
  "keywords": [
    "zf3",
    "Zend Framework 3",
    "sass",
    "scss",
    "sass compiler",
    "php compiler scss"
  ],
  "authors": [{
    "name": "Mickael TONNELIER",
    "role": "Developer"
  }],
  "bin": [
    "bin/zf-sass-compass-build",
    "bin/zf-sass-compiler"
  ],
  "require": {
    "php": "^5.5.9 || ^7.0",
    "zendframework/zend-eventmanager": "^3.0.1",
    "zendframework/zend-servicemanager": "^3.1.1",
    "zendframework/zend-mvc": "^3.0.3",
    "leafo/scssphp": "^v0.6.6"
  },
  "autoload": {
    "psr-4": {
      "zf3SassCompiler\\": "src/"
    }
  },
  "scripts": {
    "post-update-cmd": [
      "zf-sass-compass-build"
    ]
  }
}
